{
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  },
  "npm": {
    "publish": true
  },
  "git": {
    "commitMessage": "chore(release): ${version}"
  },
  "github": {
    "release": true,
    "releaseName": "${version}"
  }
}
